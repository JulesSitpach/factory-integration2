{
  "name": "trade-navigator-pro",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered SaaS platform helping US SMBs navigate trade policy and tariff volatility",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "i18n:extract": "formatjs extract 'app/**/*.{js,jsx,ts,tsx}' --out-file public/locales/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "analyze": "ANALYZE=true next build",
    "gen:env-types": "npx gen-env-types .env.local -o types/env.d.ts -e .env.example"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@stripe/react-stripe-js": "^2.3.1",
    "@stripe/stripe-js": "^2.1.10",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.38.4",
    "ai": "^2.2.20",
    "chart.js": "^4.4.0",
    "csv-parse": "^5.5.2",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "next": "14.0.0",
    "next-intl": "^3.0.0-beta.19",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.2",
    "swr": "^2.2.4",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-config-next": "14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "gen-env-types": "^1.3.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JulesSitpach/factory-integration.git"
  },
  "author": "TradeNavigatorPro Team",
  "license": "UNLICENSED",
  "keywords": [
    "trade",
    "tariff",
    "supply-chain",
    "smb",
    "saas",
    "nextjs",
    "supabase",
    "stripe",
    "ai"
  ]
}
